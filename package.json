{
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "postinstall": "typesync",
    "prepare": "husky install",
    "dev": "run-p dev:*",
    "dev:tailwind": "tailwindcss -i src/styles/tailwind.css -o src/tailwind.dist.css -w",
    "dev:eleventy": "eleventy --serve",
    "dev:vite": "vite",
    "build": "NODE_ENV=production yarn build:tailwind && yarn build:vite && yarn build:eleventy",
    "build:tailwind": "tailwindcss -i src/styles/tailwind.css -o src/tailwind.dist.css",
    "build:eleventy": "eleventy",
    "build:vite": "vite build",
    "type-check": "tsc",
    "prettier:write": "yarn prettier --write ./src/**/*.{ts,svelte}",
    "eslint": "eslint './src/**/*.{ts,svelte}' -c .eslintrc.json --fix",
    "format": "yarn prettier:write && yarn eslint"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^1.0.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^3.0.0",
    "@types/eslint": "^8.2.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^16.11.9",
    "@types/postcss-import": "^14.0.0",
    "@types/preloadjs": "^0.6.32",
    "@types/prettier": "^2.4.2",
    "@types/scroll-lock": "^2.1.0",
    "@types/stats.js": "^0.17.0",
    "@types/three": "^0.134.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-custom-media": "^8.0.0",
    "postcss-custom-properties": "^12.0.0",
    "postcss-easings": "^3.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.0.2",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.47.0",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3",
    "typesync": "^0.8.0",
    "vite": "^2.6.14",
    "vite-plugin-compression": "^0.3.6",
    "vite-plugin-glsl": "^0.0.5",
    "vite-plugin-glslify": "^1.3.0"
  },
  "dependencies": {
    "@dogstudio/highway": "^2.2.1",
    "axios": "^0.24.0",
    "date-fns": "^2.28.0",
    "detect-gpu": "^3.1.25",
    "embla-carousel": "^6.2.0",
    "lake": "https://github.com/hayakawasho/lake",
    "lazysizes": "^5.3.2",
    "lil-gui": "^0.16.1",
    "mitt": "^3.0.0",
    "neverthrow": "^4.3.1",
    "prouter": "^10.0.12",
    "ress": "^4.0.0",
    "robot3": "^0.2.21",
    "scroll-lock": "^2.1.5",
    "svelte-robot-factory": "^2.0.3",
    "three": "^0.135.0",
    "ts-pattern": "^3.3.5",
    "tween24": "^1.3.0"
  },
  "prepare": "husky install",
  "lint-staged": {
    "*.{ts,svelte}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
