{
  "name": "kuboniku",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "prepare": "husky install",
    "deploy": "next build && next export",
    "dev": "next --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "prettier:write": "yarn prettier --write ./src/**/*.{ts,tsx}",
    "eslint": "eslint './src/**/*.{ts,tsx}' -c .eslintrc.json --fix",
    "format": "yarn prettier:write && yarn eslint",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@ashthornton/asscroll": "^2.0.2",
    "@aspida/axios": "^1.7.1",
    "@react-hook/event": "^1.2.3",
    "@react-hook/intersection-observer": "^3.1.1",
    "@react-hook/mouse-position": "^4.1.0",
    "@react-hook/resize-observer": "^1.2.2",
    "@react-hook/size": "^2.1.2",
    "@react-hook/toggle": "^2.0.1",
    "@react-hook/window-size": "^3.0.7",
    "@react-three/fiber": "^7.0.1",
    "@types/classnames": "^2.3.1",
    "@types/three": "^0.127.1",
    "@unseenco/e": "^1.9.1",
    "@use-gesture/react": "^10.0.0-beta.14",
    "apollo-datasource-graphql": "^1.3.2",
    "apollo-server-micro": "^3.0.2",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "constate": "^3.3.0",
    "cors": "^2.8.5",
    "date-fns": "^2.17.0",
    "detect-gpu": "^3.1.23",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.3",
    "graphql-anywhere": "^4.2.7",
    "graphql-request": "^3.4.0",
    "gsap": "./vendors/gsap-bonus.tgz",
    "neverthrow": "^4.2.2",
    "next": "^12.0.1",
    "nextjs-basic-auth-middleware": "^0.2.0",
    "react": "18.0.0-alpha-9212d994b",
    "react-dom": "18.0.0-alpha-a8cabb564-20210915",
    "ress": "^4.0.0",
    "scroll-lock": "^2.1.4",
    "stats.js": "^0.17.0",
    "swr": "^1.0.1",
    "three": "^0.127.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.1",
    "@graphql-codegen/typescript": "2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "autoprefixer": "^10.4.0",
    "babel-plugin-macros": "^3.0.1",
    "eslint": "^7.31.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "glslify-loader": "^2.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "postcss": "^8.2.12",
    "postcss-custom-properties": "^12.0.0",
    "postcss-easings": "^3.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^9.0.0",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.4.4"
  },
  "license": "MIT",
  "prepare": "husky install",
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
