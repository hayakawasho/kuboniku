{
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "prepare": "husky install",
    "clean": "rimraf _site/* transpiled/*",
    "openapi2aspida": "npx openapi2aspida",
    "dev:jsx": "babel src/views --watch --extensions '.js,.jsx' --out-dir transpiled --copy-files",
    "dev:vite": "vite",
    "dev:11ty": "eleventy --serve",
    "dev:aspida": "aspida --watch",
    "dev": "run-p dev:*",
    "build:jsx": "babel src/views --extensions '.js,.jsx' --out-dir transpiled --copy-files",
    "build:vite": "vite build",
    "build:11ty": "eleventy",
    "build": "NODE_ENV=production run-s clean build:*",
    "preview": "yarn build && vite preview",
    "type-check": "tsc",
    "prettier:write": "yarn prettier --write ./src/**/*.{js,jsx,ts,svelte}",
    "eslint": "eslint './src/**/*.{js,jsx,ts,svelte}' -c .eslintrc.json --fix",
    "format": "yarn prettier:write && yarn eslint",
    "wp-start": "wp-env start",
    "wp-stop": "wp-env stop",
    "wp-update": "wp-env start --update"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@sveltejs/vite-plugin-svelte": "1.4.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/eslint": "^8.2.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^16.11.9",
    "@types/prettier": "^2.4.2",
    "@types/scroll-lock": "^2.1.0",
    "@types/stats.js": "^0.17.0",
    "@types/three": "^0.148.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@wordpress/env": "^4.9.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "html-react-parser": "^3.0.7",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-glslify": "^1.3.1",
    "svelte": "^3.55.1",
    "svelte-preprocess": "^5.0.0",
    "typescript": "4.7.4",
    "vite": "3.1.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-windicss": "^1.8.10",
    "windicss": "^3.5.6"
  },
  "dependencies": {
    "@aspida/fetch": "^1.11.0",
    "@dogstudio/highway": "^2.2.1",
    "axios": "0.24.0",
    "cdate": "^0.0.4",
    "detect-gpu": "^5.0.7",
    "lake": "https://github.com/hayakawasho/lake",
    "lil-gui": "^0.16.1",
    "nanostores": "^0.7.1",
    "remeda": "^1.6.0",
    "robot3": "^0.3.2",
    "scroll-lock": "^2.1.5",
    "svelte-robot-factory": "^2.0.4",
    "three": "0.110.0",
    "ts-pattern": "^4.0.6",
    "tween24": "^1.4.2"
  },
  "prepare": "husky install",
  "lint-staged": {
    "*.{ts,svelte}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
