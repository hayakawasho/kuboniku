---
import Profile from "../../_components/page/profile/index.view";
import { BREAK_POINTS } from "../../_foundation/const/mq";
import profileNameImg from "../../assets/profile.png";
import profileNameSpImg from "../../assets/profile_sp.png";
import Layout from "../../layout.astro";
---

<Layout title="Profile" permalink="/profile/" namespace="profile">
  <link
    slot="prependHead"
    as="image"
    href={profileNameImg.src}
    media={BREAK_POINTS["pc"]}
    rel="preload"
  />
  <link
    slot="prependHead"
    as="image"
    href={profileNameSpImg.src}
    media={BREAK_POINTS["sp"]}
    rel="preload"
  />
  <Profile namespace="profile" />
</Layout>
