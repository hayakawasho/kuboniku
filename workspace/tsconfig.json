{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "moduleResolution": "Node",
    "allowJs": true
  },
  "include": ["src/**/*", "@types"],
  "exclude": ["node_modules/*", "static/*", "./lib"]
}
